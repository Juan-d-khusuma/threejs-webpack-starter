{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "allowJs": true,
    "outDir": "./public",
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["./public", "./webpack"]
}
